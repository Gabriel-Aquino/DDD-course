{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-underscore-dangle": "off",
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "no-undef":"off",
      "no-unused-vars": "off",
      "semi":"off",
      "class-methods-use-this":"off"
    }
}
